table.table.table-hover.table-striped.table-condensed
  thead
    tr
      th.text-left Name
      th.text-center Value
      th.text-right Source
  tbody
    tr(ng-repeat="(name, value) in properties | publicFields")
      td.text-left {{ name }}
      td.text-center {{ value[0] }}
      td.text-right {{ value[1] }}
